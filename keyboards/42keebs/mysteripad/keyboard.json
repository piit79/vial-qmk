{
    "keyboard_name": "Mysteripad",
    "manufacturer": "42. Keebs",
    "url": "https://github.com/piit79/mysteripad",
    "maintainer": "piit79",
    "features": {
        "bootmagic": true,
        "command": false,
        "console": false,
        "extrakey": true,
        "mousekey": true,
        "encoder": true,
        "nkro": false
    },
    "processor": "atmega328p",
    "bootloader": "usbasploader",
    "usb": {
        "vid": "0x1209",
        "pid": "0x4205",
        "device_version": "0.0.1",
        "max_power": 100
    },
    "matrix_pins": {
        "cols": ["C0", "C1", "C2", "C3"],
        "rows": ["D0", "D1", "D4", "D5", "D6", "D7"]
    },
    "diode_direction": "COL2ROW",
    "encoder": {
        "rotary": [
            { "pin_a": "B0", "pin_b": "B1", "resolution": 2 },
            { "pin_a": "B1", "pin_b": "B4", "resolution": 2 },
            { "pin_a": "B4", "pin_b": "B3", "resolution": 2 },
            { "pin_a": "B3", "pin_b": "B2", "resolution": 2 }
        ]
    },
    "qmk": {
        "tap_keycode_delay": 10
    },
    "layouts": {
        "LAYOUT_numpad_6x4": {
            "layout": [
                {"matrix": [0, 0], "x": 0, "y": 0},
                {"matrix": [0, 1], "x": 1, "y": 0},
                {"matrix": [0, 2], "x": 2, "y": 0},
                {"matrix": [0, 3], "x": 3, "y": 0},

                {"matrix": [1, 0], "x": 0, "y": 1},
                {"matrix": [1, 1], "x": 1, "y": 1},
                {"matrix": [1, 2], "x": 2, "y": 1},
                {"matrix": [1, 3], "x": 2, "y": 1},

                {"matrix": [2, 0], "x": 0, "y": 2},
                {"matrix": [2, 1], "x": 1, "y": 2},
                {"matrix": [2, 2], "x": 2, "y": 2},
                {"matrix": [2, 3], "x": 3, "y": 2, "h": 2},

                {"matrix": [3, 0], "x": 0, "y": 3},
                {"matrix": [3, 1], "x": 1, "y": 3},
                {"matrix": [3, 2], "x": 2, "y": 3},

                {"matrix": [4, 0], "x": 0, "y": 4},
                {"matrix": [4, 1], "x": 1, "y": 4},
                {"matrix": [4, 2], "x": 2, "y": 4},
                {"matrix": [4, 3], "x": 3, "y": 4, "h": 2},

                {"matrix": [5, 0], "x": 0, "y": 4},
                {"matrix": [5, 1], "x": 1, "y": 4},
                {"matrix": [5, 2], "x": 2, "y": 4}
            ]
        }
    }
}
